<div>
  <p>Hello world</p>

  <table>
    <thead>
      <tr>
        <th>Nome</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= link_to(user_path(user)) { user.name } %></td>
          <td><%= user.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>